{
  "name": "le-coursier",
  "version": "0.0.1",
  "description": "Build your shopping list from the recipes you want to eat.",
  "author": "cadesalaberry",
  "scripts": {
    "start": "./node_modules/livereloadx/bin/livereloadx.js -p 8000 -s",
    "watch": "./node_modules/watchify/bin/cmd.js -d -t [ babelify --presets [ es2015 react ] ] src/app.jsx -o build/bundle.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cadesalaberry/le-coursier"
  },
  "bugs": {
    "url": "https://github.com/cadesalaberry/le-coursier/issues"
  },
  "keywords": [
    "recipe",
    "shopping list",
    "example"
  ],
  "dependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "livereloadx": "^0.3.9",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-mdl": "^1.0.1",
    "watchify": "^3.6.1",
    "snyk": "^1.25.2"
  },
  "private": true,
  "analyze": true,
  "license": "MIT",
  "snyk": true
}
